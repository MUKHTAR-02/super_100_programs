// Loops programs

#include <iostream>
using namespace std;

void helloFive() {
    for(int i=0; i<5; i++) {
        cout << "Hello World" << endl;
    }
}
 

int main() {
    // programs listing:
    cout << "---Loops" << endl;
    
   cout << "Enter 1 to print Hello World five times: " << endl;
    
    cout << endl;
    int prog;
    cout << "Enter the program number: ";
    cin >> prog;
    switch(prog) {
        case 1: 
            helloFive();
            break;
            
