// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int array(int arr[], int n) {
    cout << "enter the elements of array: ";
    for(int i=0; i<n; i++) {
        cin >> arr[i];
    }
    cout << "entered elements are: " << endl;
    for(int i=0; i<n; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;
}

int main() {
    int n = 5;
    int arr[n];
    array(arr, n);
    return 0;
}
