// Simple 10 programs

// TIPS for patterns:
// no of rows depends on i
// no of columns depends on j

#include <iostream>
using namespace std;

void print1() {
    for(int i=0; i<5; i++) {
        for(int j=0; j<=i; j++) {
            cout <<  "* ";
        }
        cout << endl;
    }
}

void print2() {
    for(int i=1; i<=5; i++) {
        for(int j=1; j<=i; j++) {
            cout <<  j << " ";
        }
        cout << endl;
    }
}

void print3() {
    for(char i='A'; i<='E'; i++) {
        for(char j='A'; j<=i; j++) {
            cout <<  j << " ";
        }
        cout << endl;
    }
}

void print4() {
    for(int i=1; i<=5; i++) {
        for(int j=1; j<=i; j++) {
            cout <<  j << " ";
        }
        cout << endl;
    }
}

void print5() {
    for(int i=1; i<=5; i++) {
        for(int j=5; j>=i; j--) {
            cout << "* " << " ";
        }
        cout << endl;
    }
}

void print6() {
    for(int i=5; i>=0; i--) {
        for(int j=1; j<=i; j++) {
            cout << j  << " ";
        }
        cout << endl;
    }
}

void print7() {
    for(char i='E'; i>='A'; i--) {
        for(char j='A'; j<=i; j++) {
            cout << j << " ";
        }
        cout << endl;
    }
}

void print8() {
    for(int i=1; i<=5; i++) {
        for(int j=i; j<=5; j++) {
            cout << j << " ";
        }
        cout << endl;
    }
}

void print9() {
    cout << "ye line print kr vani zruri thi bcoz programiz compiler m dikkat aa rhi h agr ye line print n krvai to" << endl;
    for(int i=1; i<=5; i++) {
        for(int j=5; j>i; j--) {
            cout << "  ";
        }
        for(int k=1 ; k<=i ; k++) {
            cout << "* ";
        }
        cout << endl;
    }
}

void print10() {
    cout << "line for proper code" << endl;
    for(int i=1; i<=5; i++) {
        for(int j=5; j>i; j--) {
            cout << "  ";
        }
        for(int k=1; k<=i; k++) {
            cout << k << " ";
        }
        cout << endl;
    }
}

